{
  "tracker_right_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "options_popup"
              },
              "content": {
                "type": "layout",
                "key": "item_grid_tall"
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "tabbed",
              "dock": "top",
              "tabs": [
                {
                  "title": "Main",
                  "icon": "images/master-sword.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "Main"
                    ]
                  }
                },
                {
                  "title": "Poes",
                  "icon": "images/soul.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "Poes"
                    ]
                  }
                },
                {
                  "title": "Golden Bugs",
                  "icon": "images/m-dragonfly.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "Golden Bugs"
                    ]
                  }
                },
                {
                  "title": "Shops",
                  "icon": "images/wallet.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "Shops"
                    ]
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "group",
              "dock": "bottom",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
            }
          ]
        }
      ]
    }
  }
}